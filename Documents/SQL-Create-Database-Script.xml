CREATE TABLE ACESSO (
  ID INTEGER UNSIGNED NOT NULL AUTO_INCREMENT,
  USUARIO_ID INTEGER UNSIGNED NOT NULL,
  ULTIMO_LOGIN DATETIME NULL,
  ANO_MES VARCHAR(7) NULL,
  ACESSO INTEGER UNSIGNED NULL,
  ACESSO_TOTAL INTEGER UNSIGNED NULL,
  PRIMARY KEY(ID),
  INDEX ACESSO_FKIndex1(USUARIO_ID)
);

CREATE TABLE ACESSO_HISTORICO (
  ID INTEGER UNSIGNED NOT NULL AUTO_INCREMENT,
  USUARIO_ID INTEGER UNSIGNED NOT NULL,
  DATA_HORA DATETIME NULL,
  PRIMARY KEY(ID),
  INDEX ACESSO_HISTORICO_FKIndex1(USUARIO_ID)
);

CREATE TABLE ATIVIDADE (
  ID INTEGER UNSIGNED NOT NULL AUTO_INCREMENT,
  NOME VARCHAR NULL,
  SITUACAO VARCHAR(8) NULL,
  PRIMARY KEY(ID)
);

CREATE TABLE CARGO (
  ID INTEGER UNSIGNED NOT NULL AUTO_INCREMENT,
  NOME VARCHAR(35) NULL,
  SITUACAO VARCHAR(8) NULL,
  PRIMARY KEY(ID)
);

CREATE TABLE DESEMPENHO (
  ID INTEGER UNSIGNED NOT NULL AUTO_INCREMENT,
  USUARIO_ID INTEGER UNSIGNED NOT NULL,
  ATIVIDADE_ID INTEGER UNSIGNED NOT NULL,
  PRESENCA_ID INTEGER UNSIGNED NOT NULL,
  META INTEGER UNSIGNED NULL,
  ALCANCADO FLOAT NULL,
  DESEMPENHO FLOAT NULL,
  REGISTRO DATE NULL,
  OBSERVACAO VARCHAR(200) NULL,
  CADASTRADO_POR INTEGER UNSIGNED NULL,
  ATUALIZADO_POR INTEGER UNSIGNED NULL,
  PRIMARY KEY(ID, USUARIO_ID, ATIVIDADE_ID),
  INDEX DESEMPENHO_FKIndex1(USUARIO_ID),
  INDEX DESEMPENHO_FKIndex2(ATIVIDADE_ID),
  INDEX DESEMPENHO_FKIndex3(PRESENCA_ID)
);

CREATE TABLE FEEDBACK (
  ID INTEGER UNSIGNED NOT NULL AUTO_INCREMENT,
  REMETENTE_ID INTEGER UNSIGNED NULL,
  DESTINATARIO_ID INTEGER UNSIGNED NULL,
  COMPORTAMENTAL INTEGER UNSIGNED NULL,
  PROFISSIONAL INTEGER UNSIGNED NULL,
  DESEMPENHO INTEGER UNSIGNED NULL,
  TIPO VARCHAR(10) NULL,
  FEEDBACK VARCHAR(300) NULL,
  EXIBICAO INTEGER UNSIGNED NULL,
  REGISTRO DATE NULL,
  ATUALIZADO_POR INTEGER UNSIGNED NULL,
  SITUACAO VARCHAR(10) NULL,
  PRIMARY KEY(ID)
);

CREATE TABLE GESTOR (
  ID INTEGER UNSIGNED NOT NULL AUTO_INCREMENT,
  NOME VARCHAR(50) NULL,
  SITUACAO VARCHAR(8) NULL,
  PRIMARY KEY(ID)
);

CREATE TABLE META (
  ID INTEGER UNSIGNED NOT NULL AUTO_INCREMENT,
  ATIVIDADE_ID INTEGER UNSIGNED NOT NULL,
  SETOR_ID INTEGER UNSIGNED NOT NULL,
  USUARIO_ID INTEGER UNSIGNED NOT NULL,
  META INTEGER UNSIGNED NULL,
  DESCRICAO VARCHAR(200) NULL,
  EXECUCAO DATE NULL,
  CADASTRO_EM DATE NULL,
  DESEMPENHO INTEGER UNSIGNED NULL,
  PRIMARY KEY(ID),
  INDEX META_FKIndex1(USUARIO_ID),
  INDEX META_FKIndex2(ATIVIDADE_ID),
  INDEX META_FKIndex3(SETOR_ID)
);

CREATE TABLE PERMISSAO (
  ID INTEGER UNSIGNED NOT NULL AUTO_INCREMENT,
  NOME VARCHAR(20) NULL,
  SITUACAO VARCHAR(9) NULL,
  PRIMARY KEY(ID)
);

CREATE TABLE PRESENCA (
  ID INTEGER UNSIGNED NOT NULL AUTO_INCREMENT,
  NOME VARCHAR(16) NULL,
  SITUCAO VARCHAR(8) NULL,
  PRIMARY KEY(ID)
);

CREATE TABLE REPORTAR (
  ID INTEGER UNSIGNED NOT NULL AUTO_INCREMENT,
  USUARIO_ID INTEGER UNSIGNED NOT NULL,
  MODULO VARCHAR(14) NULL,
  PRIORIDADE VARCHAR(8) NULL,
  MENSAGEM VARCHAR(300) NULL,
  SITUACAO VARCHAR(10) NULL,
  PRIMARY KEY(ID),
  INDEX REPORTAR_FKIndex1(USUARIO_ID)
);

CREATE TABLE SETOR (
  ID INTEGER UNSIGNED NOT NULL AUTO_INCREMENT,
  NOME VARCHAR(20) NULL,
  SITUACAO VARCHAR(8) NULL,
  PRIMARY KEY(ID)
);

CREATE TABLE SOLICITACAO (
  ID INTEGER UNSIGNED NOT NULL AUTO_INCREMENT,
  REMETENTE_ID INTEGER UNSIGNED NULL,
  DESTINATARIO_ID INTEGER UNSIGNED NULL,
  MENSAGEM VARCHAR(200) NULL,
  REGISTRO DATE NULL,
  SITUACAO VARCHAR(10) NULL,
  PRIMARY KEY(ID)
);

CREATE TABLE TURNO (
  ID INTEGER UNSIGNED NOT NULL AUTO_INCREMENT,
  NOME VARCHAR(10) NULL,
  SITUACAO VARCHAR(8) NULL,
  PRIMARY KEY(ID)
);

CREATE TABLE USUARIO (
  ID INTEGER UNSIGNED NOT NULL AUTO_INCREMENT,
  TURNO_ID INTEGER UNSIGNED NOT NULL,
  PERMISSAO_ID INTEGER UNSIGNED NOT NULL,
  GESTOR_ID INTEGER UNSIGNED NOT NULL,
  SETOR_ID INTEGER UNSIGNED NOT NULL,
  CARGO_ID INTEGER UNSIGNED NOT NULL,
  NOME VARCHAR(60) NULL,
  LOGIN VARCHAR(60) NULL,
  SENHA VARCHAR(32) NULL,
  EMAIL VARCHAR(50) NULL,
  SEXO CHAR NULL,
  NASCIMENTO DATE NULL,
  MATRICULA INTEGER UNSIGNED NULL,
  EFETIVACAO DATE NULL,
  CADASTRADOEM DATE NULL,
  DESLIGADO_EM DATE NULL,
  SITUACAO VARCHAR(9) NULL,
  PRIMARY KEY(ID),
  INDEX USUARIO_FKIndex2(CARGO_ID),
  INDEX USUARIO_FKIndex3(SETOR_ID),
  INDEX USUARIO_FKIndex4(GESTOR_ID),
  INDEX USUARIO_FKIndex5(PERMISSAO_ID),
  INDEX USUARIO_FKIndex5(TURNO_ID)
);


