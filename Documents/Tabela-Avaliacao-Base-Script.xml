CREATE TABLE AVAL_TIPO (
  ID INTEGER UNSIGNED NOT NULL AUTO_INCREMENT,
  TIPO VARCHAR(15) NULL,
  SITUACAO VARCHAR(8) NULL,
  PRIMARY KEY(ID)
);

CREATE TABLE AVAL_TIPO_PERGUNTA (
  ID INTEGER UNSIGNED NOT NULL AUTO_INCREMENT,
  AVAL_TIPO_ID INTEGER UNSIGNED NOT NULL,
  CLASSIFICACAO VARCHAR(15) NULL,
  SITUACAO VARCHAR(8) NULL,
  PRIMARY KEY(ID),
  INDEX AVAL_TIPO_PERGUNTA_FKIndex1(AVAL_TIPO_ID)
);

CREATE TABLE AVAL_PERGUNTA (
  ID INTEGER UNSIGNED NOT NULL AUTO_INCREMENT,
  AVAL_TIPO_PERGUNTA_ID INTEGER UNSIGNED NOT NULL,
  CARGO_ID INTEGER UNSIGNED NOT NULL,
  ORDEM INTEGER UNSIGNED NULL,
  PERGUNTA VARCHAR(350) NULL,
  SITUACAO VARCHAR(8) NULL,
  PRIMARY KEY(ID),
  INDEX AVAL_PERGUNTA_FKIndex1(CARGO_ID),
  INDEX AVAL_PERGUNTA_FKIndex3(AVAL_TIPO_PERGUNTA_ID)
);

CREATE TABLE AVAL_RESPOSTA (
  ID INTEGER UNSIGNED NOT NULL AUTO_INCREMENT,
  RESPOSTA VARCHAR(25) NULL,
  SITUACAO VARCHAR(8) NULL,
  PRIMARY KEY(ID)
);

CREATE TABLE AVAL_PERGUNTA_COM (
  ID INTEGER UNSIGNED NOT NULL AUTO_INCREMENT,
  AVAL_TIPO_ID INTEGER UNSIGNED NOT NULL,
  PERGUNTA VARCHAR(250) NULL,
  SITUACAO VARCHAR(8) NULL,
  PRIMARY KEY(ID),
  INDEX AVAL_PERGUNTA_COM_FKIndex1(AVAL_TIPO_ID)
);



CREATE TABLE AVAL_INDICE (
  ID INTEGER UNSIGNED NOT NULL AUTO_INCREMENT,
  USUARIO_ID INTEGER UNSIGNED NOT NULL,
  REGISTRO DATE NULL,
  SITUACAO VARCHAR(12) NULL,
  PRIMARY KEY(ID),
  INDEX AVAL_INDICE_FKIndex1(USUARIO_ID)
);

CREATE TABLE AVAL_REALIZADA (
  ID INTEGER UNSIGNED NOT NULL AUTO_INCREMENT,
  AVAL_INDICE_ID INTEGER UNSIGNED NOT NULL,
  AVAL_PERGUNTA_ID INTEGER UNSIGNED NOT NULL,
  AVAL_RESPOSTA_ID INTEGER UNSIGNED NOT NULL,
  AVAL_NUM INTEGER UNSIGNED NULL,
  REGISTRO DATE NULL,
  PRIMARY KEY(ID),
  INDEX AVAL_REALIZADA_FKIndex1(AVAL_PERGUNTA_ID),
  INDEX AVAL_REALIZADA_FKIndex2(AVAL_RESPOSTA_ID),
  INDEX AVAL_REALIZADA_FKIndex3(AVAL_INDICE_ID)
);

CREATE TABLE AVAL_COMENTARIO (
  ID INTEGER UNSIGNED NOT NULL AUTO_INCREMENT,
  AVAL_INDICE_ID INTEGER UNSIGNED NOT NULL,
  AVAL_PERGUNTA_COM_ID INTEGER UNSIGNED NOT NULL,
  AVAL_NUM INTEGER UNSIGNED NULL,
  COMENTARIO VARCHAR(1000) NULL,
  PRIMARY KEY(ID),
   INDEX AVAL_COMENTARIO_FKIndex1(AVAL_INDICE_ID),
  INDEX AVAL_COMENTARIO_FKIndex2(AVAL_PERGUNTA_COM_ID)
 
);

