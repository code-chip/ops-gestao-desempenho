{
    "name": "code-chip/ops-gestao-desempenho",
    "description": "Performance Management project",
    "authors": [
        {
            "name": "Code Chip",
            "email": "willvix@outlook.com"
        }
    ],
    "require": {
        "symfony/security": "^3.2",
        "aura/autoload": "^2.0",
        "suin/phpcs-psr4-sniff": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^12.2",
        "phpstan/phpstan": "^2.1"
    }
}
